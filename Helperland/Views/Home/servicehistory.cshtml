                                                                <option value="1915">1915</option>
                                                                <option value="1914">1914</option>
                                                                <option value="1913">1913</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <hr class="small">

                                        <button class="btn btn btn-primary" type="submit" onclick="updateUserDetails();">Save</button>
                                    </form>

                                </div>
                                <div class="tab-pane fade" id="address" role="tabpanel" aria-labelledby="address-tab">
                                    <div id="useraddresses">  </div>
                                </div>
                                <div class="tab-pane fade" id="password" role="tabpanel" aria-labelledby="password-tab">
                                    <form>
                                        <div class="row">
                                            <div class="col-sm-4">
                                                <div class="form-group">
                                                    <label class="control-label">Old Password</label>
                                                    <input class="form-control" formcontrolname="oldpwd" maxlength="20" id="pwd" required type="password" placeholder="Current Password" data-placeholder="Current Password">
                                                </div>
                                            </div>
                                            <div formgroupname="password">
                                                <div class="col-sm-4 col-sm-offset-8 col-sm-pull-8">
                                                    <div class="form-group">
                                                        <label class="control-label">New Password</label>
                                                        <input class="form-control" formcontrolname="pwd" maxlength="20" id="newPassword" required type="password" placeholder="Password" data-placeholder="Password">
                                                    </div>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="form-group">
                                                        <label class="control-label">Confirm Password</label>
                                                        <input class="form-control" formcontrolname="confirmPwd" maxlength="20" id="confirmPwd" required type="password" placeholder="Confirm Password" data-placeholder="Confirm Password">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <button class="btn btn btn-primary" type="submit" onclick="changepwd();">Save</button>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

        </section>
        </div>

    </main>

